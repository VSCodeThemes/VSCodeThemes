{
  "name": "@vscodethemes/web",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "main": "build/index.js",
  "scripts": {
    "build": "remix build",
    "deploy": "npm run build && wrangler publish",
    "dev:remix": "remix watch",
    "dev:miniflare": "cross-env NODE_ENV=development miniflare ./build/index.js --watch --kv-persist redis://localhost:6379 --kv VSCODETHEMES_EXTENSIONS",
    "dev": "remix build && run-p dev:*",
    "start": "cross-env NODE_ENV=production miniflare ./build/index.js",
    "lint": "eslint 'app/**/*.{js,ts,tsx}' --quiet --fix",
    "format": "prettier --write .",
    "test": "jest",
    "generate-wrangler-toml-dev": "envsub --all --env INTERNAL_API_URL=http://localhost:3000 wrangler.toml.template wrangler.toml",
    "generate-wrangler-toml": "envsub wrangler.toml.template wrangler.toml"
  },
  "dependencies": {
    "@remix-run/cloudflare": "^1.4.0",
    "@remix-run/cloudflare-workers": "^1.4.0",
    "@remix-run/react": "^1.4.0",
    "@vscodethemes/utilities": "^0.1.14",
    "colord": "^2.9.2",
    "cross-env": "^7.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.4.0",
    "@cloudflare/wrangler": "^1.19.11",
    "@miniflare/storage-redis": "^2.4.0",
    "@remix-run/dev": "^1.4.0",
    "@remix-run/eslint-config": "^1.4.0",
    "@types/jest": "^27.4.1",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "envsub": "^4.0.7",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^27.5.1",
    "miniflare": "^2.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "ts-jest": "^27.1.4",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=14"
  }
}
